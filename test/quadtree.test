xBound = 1000
yBound = 1000

q = new QuadTree(xBound,yBound,10)

testAddRemove = () ->
	points = {} 
	for id in [0...100]
		p = Vector2D.randomVector(xBound, yBound)
		points[id] = p 
		q.addObject(id, p)
	for id in [0...50]
		q.removeObject(id)
	console.log "No issues detected, nPoints: ", q.numPoints

testAddRemove()